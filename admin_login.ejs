<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Login</title>

    <!-- Fontfaces CSS-->
    <link href="adminAssest/css/font-face.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="adminAssest/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- adminAssest/adminAssest/vendor/ CSS-->
    <link href="adminAssest/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor//bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="adminAssest/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="adminAssest/css/theme.css" rel="stylesheet" media="all">

    <!-- SweetAlert2 CSS -->
    <link href="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.20/dist/sweetalert2.min.css" rel="stylesheet">

</head>

<body class="animsition">
    <div class="page-wrapper">
        <div class="page-content--bge5">
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">
                        <div class="login-logo">
                            <a href="#">
                                <Assest/ src="adminAssest/go.png" alt="CoolAdmin">
                            </a>
                        </div>
                        <div class="login-form">
                            <br/>
                            <center><p style="color:red;"><%= msg %></p></center> <br/>
                            <br/>
                            <form action="/admin/admin_loginchacked" method="post">
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <input class="au-input au-input--full" type="email" name="email" placeholder="Email">
                                </div>
                                <div class="form-group">
                                    <label>Password</label>
                                    <input class="au-input au-input--full" type="password" name="password" placeholder="Password">
                                </div>
                                <div class="login-checkbox">
                                    <label>
                                        <input type="checkbox" name="remember">Remember Me
                                    </label>
                                    <label>
                                        <a href="#">Forgotten Password?</a>
                                    </label>
                                </div>
                                <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">sign in</button>
                                <div class="social-login-content">
                                    <div class="social-button">
                                        <button class="au-btn au-btn--block au-btn--blue m-b-20">sign in with facebook</button>
                                        <button class="au-btn au-btn--block au-btn--blue2">sign in with twitter</button>
                                    </div>
                                </div>
                            </form>
                            <div class="register-link">
                                <p>
                                    Don't you have account?
                                    <a href="#">Sign Up Here</a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery JS-->
    <script src="adminAssest/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="adminAssest/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="adminAssest/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- adminAssest/adminAssest/vendor/ JS       -->
    <script src="adminAssest/vendor/slick/slick.min.js"></script>
    <script src="adminAssest/vendor/wow/wow.min.js"></script>
    <script src="adminAssest/vendor/animsition/animsition.min.js"></script>
    <script src="adminAssest/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="adminAssest/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="adminAssest/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="adminAssest/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="adminAssest/vendor//perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="adminAssest/adminAssest/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="adminAssest/vendor/select2/select2.min.js"></script>

    <!-- Main JS-->
    <script src="adminAssest/js/main.js"></script>

    <!-- SweetAlert2 JS -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.20/dist/sweetalert2.min.js"></script>

    <!-- Custom JS to trigger SweetAlert -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            var msg = "<%= msg %>"; // Retrieve the msg variable passed from the server (e.g., failed login message)

            if (msg) {
                Swal.fire({
                    icon: 'error',
                    title: 'Oops...',
                    text: msg
                });
            }
            
        });
    </script>

</body>

</html>

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <%-include('admin_haderBar')%>
            <!-- END HEADER DESKTOP-->

            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
            
                        <div class="row">
                            <div class="col-md-12">
                                <!-- DATA TABLE -->
                                <h3 class="title-5 m-b-35">data table</h3>
                                <div class="table-data__tool">
                                    <div class="table-data__tool-left">
                                        <div class="rs-select2--light rs-select2--md">
                                            <select class="assest//js-select2" name="property">
                                                <option selected="selected">All Properties</option>
                                                <option value="">Option 1</option>
                                                <option value="">Option 2</option>
                                            </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                        <div class="rs-select2--light rs-select2--sm">
                                            <select class="assest//js-select2" name="time">
                                                <option selected="selected">Today</option>
                                                <option value="">3 Days</option>
                                                <option value="">1 Week</option>
                                            </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                        <button class="au-btn-filter">
                                            <i class="zmdi zmdi-filter-list"></i>filters</button>
                                    </div>
                                    <div class="table-data__tool-right">
                                        <button class="au-btn au-btn-icon au-btn--green au-btn--small">
                                            <i class="zmdi zmdi-plus"></i>add item</button>
                                        <div class="rs-select2--dark rs-select2--sm rs-select2--dark2">
                                            <select class="assest//js-select2" name="type">
                                                <option selected="selected">Export</option>
                                                <option value="">Option 1</option>
                                                <option value="">Option 2</option>
                                            </select>
                                            <div class="dropDownSelect2"></div>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>LName</th>
                                                <th>Gender</th>
                                                <th>Contact</th>
                                                <th>Address</th>
                                                <th>State</th>
                                                <th>City</th>
                                                <th>email</th>
                                                <th>Password</th>
                                                <th>Image</th>
                                                <th>status</th>
                                                <th></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% data.map(function(item,index){%>       
                                                <tr>
                                                   <td><%=item.name%></td>
                                                   <td><%=item.lname%></td>
                                                   <td><%=item.gender%></td>
                                                   <td><%=item.contact%></td>
                                                   <td><%=item.address%></td>
                                                   <td><%=item.state%></td>
                                                   <td><%=item.city%></td>
                                                   <td><%=item.email%></td>
                                                   <td><%=item.password%></td>
                                                   <td><img class="Assest//-circle" src="/uploads/<%=item.image%>" width="35px" height="35px" align=""></td>
                                                   <!-- <td>status</td> -->
                                                   <td>
                                                    <a href="/admin/userview_Data?u_id=<%=item.user_id%>">
                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="Send">
                                                        <i class="zmdi zmdi-mail-send"></i>
                                                        </button>
                                                    </a>
                                                  
                                                    <a href="/admin/userselect_Data?edit_id=<%=item.user_id%>">
                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="Edit">
                                                            <i class="zmdi zmdi-edit"></i>
                                                        </button>
                                                    </a>
                                                     
                                                    <a href="/admin/userdelete_Data?edit_id=<%=item.user_id%>" onclick="return confirm('Are you sure you want to delete this user?');">
                                                        <button class="item" data-toggle="tooltip" data-placement="top" title="Delete">
                                                            <i class="zmdi zmdi-delete"></i>
                                                        </button>
                                                    </a>

                                                   </td>
                                                   </tr>
                                                   <%
                                                  });
                                              %> 
                                        </tbody>
                                    </table>
                                </div>
                                <!-- END DATA TABLE -->
                            </div>
                        </div>
                        

                        <!-- Footer start -->
                       <%-include('admin_footerBar')%>
                       <!-- Footer end -->
                        
                    </div>
                </div>
            </div>
